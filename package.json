{
  "name": "the-arch-co-website",
  "description": "The website for The Arch Co.",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "next": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap && node ./scripts/generators/html-sitemap.mjs && node ./scripts/duplicate-redirect-lint.mjs",
    "start": "next start",
    "lint": "concurrently pnpm:lint:*",
    "lint:next": "next lint --fix",
    "lint:styles": "stylelint \"**/*.{css,scss}\" --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "concurrently pnpm:test:*",
    "test:storybook": "test-storybook --coverage",
    "test:ts": "tsc",
    "chromatic": "chromatic --exit-zero-on-changes",
    "gen": "concurrently pnpm:gen:*",
    "gen:docs": "node ./scripts/generators/docs.mjs",
    "gen:icons": "node ./scripts/generators/icons.mjs",
    "gen:favicons": "node ./scripts/generators/favicons.mjs",
    "gen:scssexports": "node ./scripts/generators/scss-exports.mjs",
    "watch": "concurrently pnpm:watch:*",
    "watch:docs": "pnpm run gen:docs --watch",
    "watch:scssexports": "pnpm run gen:scssexports --watch",
    "watch:icons": "pnpm run gen:icons --watch",
    "watch:favicons": "pnpm run gen:favicons --watch",
    "devkit": "node ./scripts/devkit.js",
    "prettier-write": "prettier --write ."
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.6",
    "@googlemaps/markerclusterer": "^2.5.3",
    "@hookform/resolvers": "^3.1.1",
    "@next/bundle-analyzer": "^14.2.5",
    "@next/third-parties": "^14.2.4",
    "@popperjs/core": "^2.11.8",
    "@portabletext/react": "^3.1.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@sanity/image-url": "^1.0.2",
    "@sanity/react-loader": "^1.10.3",
    "@sanity/visual-editing": "^2.1.6",
    "@vercel/analytics": "^1.5.0",
    "@vis.gl/react-google-maps": "^1.0.2",
    "axios": "^1.4.0",
    "bowser": "^2.11.0",
    "browser-update": "^3.3.53",
    "date-fns": "^4.1.0",
    "next": "^14.2.4",
    "next-auth": "^4.24.7",
    "next-sanity": "^9.4.2",
    "next-sitemap": "^4.2.3",
    "react": "^18.3.1",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.52.0",
    "react-icons": "^4.7.1",
    "react-popper": "^2.3.0",
    "react-redux": "^8.0.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@chromatic-com/storybook": "^1",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@next/eslint-plugin-next": "^13.1.6",
    "@storybook/addon-a11y": "^8.1.10",
    "@storybook/addon-coverage": "^1.0.4",
    "@storybook/addon-essentials": "^8.1.10",
    "@storybook/addon-interactions": "^8.1.10",
    "@storybook/addon-links": "^8.1.10",
    "@storybook/addon-storysource": "^8.1.10",
    "@storybook/manager-api": "^8.1.10",
    "@storybook/nextjs": "^8.1.10",
    "@storybook/preview-api": "^8.1.10",
    "@storybook/react": "^8.1.10",
    "@storybook/test": "^8.1.10",
    "@storybook/test-runner": "^0.18.2",
    "@storybook/theming": "^8.1.10",
    "@types/google.maps": "^3.55.9",
    "@types/jest": "^29.5.1",
    "@types/node": "18.11.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-redux": "^7.1.25",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "axe-playwright": "^1.2.3",
    "blessed": "^0.1.81",
    "blessed-xterm": "^1.4.4",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "chromatic": "^11.5.3",
    "classnames": "^2.3.2",
    "concurrently": "^9.0.1",
    "eslint": "8.27.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-output": "^3.0.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdownlint": "^0.4.1",
    "eslint-plugin-mdx": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.8.0",
    "favicons": "^7.1.0",
    "full-icu": "^1.5.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.4.0",
    "jest-html-reporter": "^3.7.0",
    "lint-staged": "^13.1.0",
    "markdown-loader": "^8.0.0",
    "markdownlint": "^0.27.0",
    "markdownlint-cli": "^0.33.0",
    "mini-css-extract-plugin": "^2.7.2",
    "msw": "^2.3.1",
    "msw-storybook-addon": "^2.0.2",
    "next-images": "^1.8.4",
    "next-purgecss": "^4.0.0",
    "next-redux-wrapper": "^8.1.0",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.21",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^9.6.0",
    "prettier": "^2.7.1",
    "pretty": "^2.0.0",
    "react-docgen-typescript": "^2.2.2",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.60.0",
    "sass-loader": "^13.2.1",
    "sb-addon-permutation-table": "^0.1.3",
    "sharp": "^0.33.4",
    "storybook": "^8.1.10",
    "style-loader": "^3.3.2",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^29.0.5",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^5.4.5",
    "uuid": "^9.0.0",
    "wait-on": "^8.0.1",
    "webpack": "5.75.0",
    "yup": "^1.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,mdx}": [
      "prettier --write",
      "pnpm gen",
      "pnpm lint:next"
    ],
    "*.{scss,css}": [
      "prettier --write",
      "pnpm gen:scssexports",
      "pnpm lint:styles"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "browserslist": [
    "defaults and fully supports es6-module"
  ],
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.15.0"
}
